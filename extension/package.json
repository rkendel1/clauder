{
	"name": "kuhmpel",
	"displayName": "Kuhmpel",
	"description": "Kuhmpel: Your AI pair programmer in VSCode. Supports multiple AI providers including Anthropic, OpenAI, Google, and more. Create/edit files and execute commands with AI guidance. Perfect for no-coders to senior devs. Transform your IDE into a collaborative AI workspace.",
	"version": "2.3.13",
	"icon": "assets/icon.png",
	"engines": {
		"vscode": "^1.96.0"
	},
	"author": {
		"name": "rkendel1"
	},
	"license": "AGPL-3.0-or-later",
	"publisher": "rkendel1",
	"repository": {
		"type": "git",
		"url": "https://github.com/rkendel1/clauder"
	},
	"categories": [
		"AI",
		"Programming Languages",
		"Snippets",
		"Machine Learning",
		"Education",
		"Chat"
	],
	"keywords": [
		"ai",
		"code completion",
		"coding agent",
		"coding assistant",
		"no code ai",
		"autonomous coding",
		"ai pair programmer",
		"agentic",
		"no-code",
		"productivity",
		"automation",
		"multi-provider",
		"anthropic",
		"openai",
		"google",
		"deepseek",
		"mistral",
		"ai assistant",
		"code generation",
		"chatgpt",
		"copilot",
		"github",
		"json",
		"jsonc",
		"keybindings",
		"aider",
		"codegpt"
	],
	"activationEvents": [
		"onUri"
	],
	"handlers": {
		"onUri": {
			"uriHandler": "./dist/extension.js"
		}
	},
	"main": "./dist/extension.js",
	"type": "commonjs",
	"contributes": {
		"viewsContainers": {
			"activitybar": [
				{
					"id": "kuhmpel-coder-main-ActivityBar",
					"title": "Kuhmpel AI Coder",
					"icon": "assets/kodu-logo.svg"
				}
			]
		},
		"views": {
			"kuhmpel-coder-main-ActivityBar": [
				{
					"type": "webview",
					"id": "kuhmpel-coder-main.SidebarProvider",
					"name": ""
				}
			]
		},
		"commands": [
			{
				"command": "kuhmpel-coder-main.setApiKey",
				"title": "Kuhmpel: Set API Key"
			},
			{
				"command": "extension.openExternalLink",
				"title": "Open External Link"
			},
			{
				"command": "kuhmpel-coder-main.plusButtonTapped",
				"title": "New Task",
				"icon": "$(add)"
			},
			{
				"command": "kuhmpel-coder-main.historyButtonTapped",
				"title": "History",
				"icon": "$(history)"
			},
			{
				"command": "kuhmpel-coder-main.popoutButtonTapped",
				"title": "Open in Editor",
				"icon": "$(link-external)"
			},
			{
				"command": "kuhmpel-coder-main.settingsButtonTapped",
				"title": "Settings",
				"icon": "$(settings-gear)"
			},
			{
				"command": "kuhmpel-coder-main.openInNewTab",
				"title": "Open In New Tab",
				"category": "Kuhmpel"
			}
		],
		"menus": {
			"view/title": [
				{
					"command": "kuhmpel-coder-main.plusButtonTapped",
					"group": "navigation@1",
					"when": "view == kuhmpel-coder-main.SidebarProvider"
				},
				{
					"command": "kuhmpel-coder-main.historyButtonTapped",
					"group": "navigation@2",
					"when": "view == kuhmpel-coder-main.SidebarProvider"
				},
				{
					"command": "kuhmpel-coder-main.popoutButtonTapped",
					"group": "navigation@3",
					"when": "view == kuhmpel-coder-main.SidebarProvider"
				},
				{
					"command": "kuhmpel-coder-main.settingsButtonTapped",
					"group": "navigation@4",
					"when": "view == kuhmpel-coder-main.SidebarProvider"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run package",
		"compile": "npm run check-types && npm run lint && tsx esbuild.ts",
		"watch": "npm-run-all -p watch:*",
		"watch:esbuild": "tsx esbuild.ts --watch",
		"install:all": "npm install && cd webview-ui-vite && npm install",
		"install:clean": "npm run clean && npm run install:all",
		"clean": "rm -rf out && rm -rf dist && rm -rf node_modules && rm -rf webview-ui-vite/dist && rm -rf webview-ui-vite/node_modules",
		"watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
		"package": "npm run build:webview && npm run check-types && npm run lint && tsx esbuild.ts --production",
		"pretest": "npm run lint",
		"check-types": "tsc --noEmit",
		"build": "npx vsce package --no-dependencies",
		"publish": "npx vsce publish --no-dependencies",
		"publish:pre-release": "npx vsce publish --pre-release --no-dependencies",
		"lint": "eslint src",
		"test": "vscode-test",
		"dev": "tsx esbuild.ts --watch",
		"start:webview": "cd webview-ui-vite && npm run dev",
		"build:webview": "cd webview-ui-vite && npm run build",
		"test:webview": "cd webview-ui-vite && npm run test",
		"db:migrate": "drizzle-kit migrate",
		"db:generate": "drizzle-kit generate"
	},
	"devDependencies": {
		"@jest/globals": "^29.7.0",
		"@types/archiver": "^6.0.3",
		"@types/chai": "^5.0.1",
		"@types/diff": "^5.2.3",
		"@types/diff-match-patch": "^1.0.36",
		"@types/jest": "^29.5.14",
		"@types/lodash": "^4.17.15",
		"@types/mocha": "^10.0.10",
		"@types/node": "~20.17.19",
		"@types/sanitize-html": "^2.13.0",
		"@types/turndown": "^5.0.5",
		"@types/vscode": "^1.96.0",
		"@types/write-file-atomic": "^4.0.3",
		"@typescript-eslint/eslint-plugin": "^8.24.1",
		"@typescript-eslint/parser": "^8.24.1",
		"@vitejs/plugin-react-swc": "^3.8.0",
		"@vscode/test-cli": "^0.0.10",
		"@vscode/test-electron": "^2.4.1",
		"@vscode/vsce": "^3.2.2",
		"drizzle-kit": "^0.30.4",
		"esbuild": "0.25.2",
		"eslint": "^9.20.1",
		"tar": "^7.4.3",
		"tsx": "^4.19.3",
		"typescript": "^5.7.3"
	},
	"dependencies": {
		"@ai-sdk/amazon-bedrock": "^2.0.4",
		"@ai-sdk/anthropic": "^1.2.9",
		"@ai-sdk/deepseek": "^0.1.12",
		"@ai-sdk/fireworks": "^0.1.12",
		"@ai-sdk/google": "^1.2.10",
		"@ai-sdk/google-vertex": "^2.2.14",
		"@ai-sdk/mistral": "^1.1.13",
		"@ai-sdk/openai": "^1.1.14",
		"@ai-sdk/openai-compatible": "^0.2.9",
		"@ai-sdk/provider": "^1.1.2",
		"@amplitude/analytics-browser": "^2.11.12",
		"@amplitude/analytics-node": "^1.3.6",
		"@anthropic-ai/sdk": "^0.26.1",
		"@ewoudenberg/difflib": "^0.1.0",
		"@libsql/client": "^0.14.0",
		"@openrouter/ai-sdk-provider": "https://github.com/kodu-ai/ai-sdk-provider-openrouter.git",
		"@types/clone-deep": "^4.0.4",
		"@types/pdf-parse": "^1.1.4",
		"@vscode/codicons": "^0.0.36",
		"ai": "^4.3.5",
		"ansi-regex": "^6.1.0",
		"archiver": "^7.0.1",
		"atomically": "^2.0.3",
		"axios": "^1.7.9",
		"chai": "^5.2.0",
		"clone-deep": "^4.0.1",
		"dedent": "^1.5.3",
		"default-shell": "^2.2.0",
		"delay": "^6.0.0",
		"diff": "^5.2.0",
		"diff-match-patch": "^1.0.5",
		"dotenv": "^16.4.7",
		"drizzle-orm": "^0.38.4",
		"execa": "^9.5.2",
		"globby": "^14.1.0",
		"image-size": "^1.2.0",
		"isbinaryfile": "^5.0.4",
		"jest": "^29.7.0",
		"jotai": "^2.12.1",
		"lodash": "^4.17.21",
		"mammoth": "^1.9.0",
		"mocha": "^11.1.0",
		"nanoid": "^5.1.0",
		"openai": "^4.85.2",
		"os-name": "^6.0.0",
		"p-queue": "^8.1.0",
		"p-wait-for": "^5.0.2",
		"pdf-parse": "^1.1.1",
		"puppeteer-chromium-resolver": "^23.0.0",
		"puppeteer-core": "^23.11.1",
		"rxjs": "^7.8.1",
		"sanitize-html": "^2.14.0",
		"serialize-error": "^11.0.3",
		"string-similarity-js": "^2.1.4",
		"strip-ansi": "^7.1.0",
		"tree-kill": "^1.2.2",
		"tree-sitter-wasms": "^0.1.12",
		"turndown": "^7.2.0",
		"web-tree-sitter": "^0.22.6",
		"write-file-atomic": "^6.0.0",
		"zod": "^3.24.2"
	}
}
